    <script>
        $(document).ready(function () {
            $(".ajs-message").removeClass('ajs-visible');
            
            // Stop user to press enter in textbox
            $("input:text").keypress(function (event) {
                if (event.keyCode == 13) {
                    event.preventDefault();
                    return false;
                }
            });
        });
        $(window).load(function () {
            $(".loader").fadeOut("1000");
        })
        $(document)
            .ajaxStart(function () {
                $('.loader').show();
            })
            .ajaxStop(function () {
                $('.loader').hide();
            });


        function readCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return null;
        }

        function createCookie(name, value, days) {
            if (days) {
                var date = new Date();
                //date.setTime(date.getTime() + (days  24  60  60  1000));
                var date1 = new Date(date.getTime() + (86400000 * (days)));
                var expires = "; expires=" + date1.toGMTString();
            }
            else var expires = "";
            document.cookie = name + "=" + value + expires + "; path=/";
        }

        function deleteCookie(name) {
            document.cookie = name + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        }

        function errorAlert(errorMessage) {
            alertify.error(errorMessage + " <div class='close-icon closeAlertify'></div>", setTimeout(function () { }, 50000));
        }
    </script>

